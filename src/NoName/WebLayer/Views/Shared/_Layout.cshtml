<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - HolyStream</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

<ul >
    <li>@Html.ActionLink("Files", "Index", "MediaFile")</li>
    <li>@Html.ActionLink("Playlists", "Index", "Playlist")</li>
    <li>@Html.ActionLink("Logs", "Index", "Logs")</li>
</ul>

<div class="container body-content">
    @RenderBody()
    <hr/>
    <footer class="text-center">
        <div class="container">
            <div class="row">
                <p>HolySquad © 2014 - @DateTime.Now.Year</p>
            </div>
        </div>
    </footer>
</div>

</body>
</html>