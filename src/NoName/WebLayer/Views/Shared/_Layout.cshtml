<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - HolyStream</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link href="~/Scripts/360-player/360player.css" rel="stylesheet" />
    <link href="~/Scripts/360-player/360player-visualization.css" rel="stylesheet" />
    <link href="~/Scripts/360-player/demo-slider-controls.css" rel="stylesheet" />

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    @*<script src="~/Scripts/jquery-1.10.2.intellisense.js"></script>*@
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/respond.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/player/soundmanager2.js"></script>
  

</head>
<body>
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            @Html.ActionLink("HolyStream", "Index", "Home", new {area = ""}, new {@class = "navbar-brand"})
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Files", "Index", "MediaFile")</li>
                <li>@Html.ActionLink("Logs", "Index", "Logs")</li>
                <li>@Html.ActionLink("MediaFilesLog", "MediaLog", "Logs")</li>
           <!--     <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>  -->
            </ul>
            @Html.Partial("_LoginPartial")
        </div>
    </div>
</div>


<div class="container body-content">
    @RenderBody()
    <hr/>
    <footer class="text-center">
        <div class="container">
            <div class="row">
                <div class="social_icon">
                    <a href="#" class="fa fa-facebook"></a>
                    <a href="#" class="fa fa-twitter"></a>
                    <a href="#" class="fa fa-linkedin"></a>
                    <a href="#" class="fa fa-tumblr"></a>
                </div>
                <p>HolySquad © 2014 - @DateTime.Now.Year</p>
            </div>
        </div>
    </footer>
</div>

    <script src="~/Scripts/360-player/script/360player.js"></script>

    <script src="~/Scripts/360-player/script/berniecode-animator.js"></script>

    <script src="~/Scripts/360-player/script/excanvas.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap") 

    <script src="~/Scripts/360-player/script/360player.js"></script>

    <script src="~/Scripts/360-player/script/berniecode-animator.js"></script>

    <script src="~/Scripts/360-player/script/excanvas.js"></script>
    @RenderSection("scripts", false)

  

</body>
</html>